@model  ProductsListViewModel

@foreach (var product in Model.Products)
{
    @await Html.PartialAsync("ProductSummary", product)

}

<div page-model="@Model.PagingInfo" page-action="List" page-url-category="@Model.CurrentCategory" page-classes-enabled="true" page-class="btn" page-class-normal="btn-secondary"
     page-class-selected="btn-primary" class="btn-group pull-right m-1"></div>